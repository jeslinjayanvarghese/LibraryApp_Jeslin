					<input type="password" name="Password" id="pd" placeholder="Password" onkeyup="return passwordChanged();" required="" />
					<div id="StrengthDisp"></div>
					<input type="password" name="Password" id="cpd" placeholder="Confirm Password" onkeyup="return pass2();" required="" />
					<div id="cpdHelp"></div>
				<div class="submit-w3l">
					<input type="submit" id="regbtn" value="Sign up">
				</div>

		<div class="form-w3-agile">
			<h2 class="sub-agileits-w3layouts">Sign Up</h2>
			<form action="/login" method="get" onsubmit="return validate();" >
				    <input type="email" name="Eamil" id="email" placeholder="Email" onkeyup="return emailvalidate();" required="" />
					<div id="emailHelp"></div>
					<input type="text" name="Username" id="uname" pattern="[A-za-z]+" placeholder="Username" required="" />
